  "tokenColors": [
    {
      "scope": "comment",
      "settings": {
        "foreground": "#747c8b",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "keyword",
        "storage.type",
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#D08770"
      }
    },
    {
      "scope": "string",
      "settings": {
        "foreground": "#A3BE8C"
      }
    },
    {
      "scope": [
        "variable",
        "entity.name.function"
      ],
      "settings": {
        "foreground": "#8FBCBB"
      }
    },
    {
      "scope": [
        "variable.other.readwrite",
        "variable.parameter",
        "support.variable",
        "keyword.operator"
      ],
      "settings": {
        "foreground": "#BBC3D4"
      }
    },
    {
      "scope": [
        "variable.other.constant",
        "constant.numeric"
      ],
      "settings": {
        "foreground": "#BE9DB8"
      }
    },
    {
      "scope": [
        "entity.name.type",
        "support.class",
        "support.type"
      ],
      "settings": {
        "foreground": "#EBCB8B"
      }
    }
  ]
